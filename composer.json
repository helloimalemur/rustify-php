{
  "name": "helloimalemur/rustify-php",
  "description": "Rust-like Option and Result types for PHP",
  "type": "library",
  "require": {
    "php": ">=8.0"
  },
  "autoload": {
    "psr-4": {
      "Rustify\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:coverage": "vendor/bin/phpunit --coverage-text"
  }
}
